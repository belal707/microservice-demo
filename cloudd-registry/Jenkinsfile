node 
{
  checkout scm
	dir('cloudd-registry')
	{
	  stage('Package') 
	  {
	     sh 'mvn clean package -DskipTests'
	  }
	}
  	dir('cloudd-registry')
	{
  		stage('Create Docker Image') 
  		{
    			docker.build("belalansari/testapi1")
  		}
	}
	dir('cloudd-registry')
	{
		stage('Run Tests') 
  		{
   		try 
		{  
			sh "mvn test"
        		docker.build("belalansari/testapi1").push()
    		} catch (error)
		{

    		} 
	}
}
